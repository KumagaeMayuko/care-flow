{% for value in tree %}
    <input type="checkbox" name="ctg_id" value="{{value.id}}"
    {% if dataArr.ctg_name == value.ctg_name %} checked="checked" {% endif %}>
    {{value.ctg_name}}
    {% if value.children is defined %}
        {% include 'common/view/category_list.html.twig' with {'tree': value.children} %}
    {% endif %}
{% endfor %}


